{
  "expo": {
    "name": "live-figure",
    "slug": "live-figure",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access photo library.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access photo library.",
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access photo library.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera.",
        }
      ]
    ],
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.printii.livefigure"
    },
    "ios": {
      "bundleIdentifier": "com.printii.livefigure"
    }
  },
  "name": "live-figure"
}
